                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="confirm_date">
                <property name="data_type">date</property>
                <property name="model_attribute">confirm_date</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
              </object>
              <packing>
                <property name="left_attach">3</property>
                <property name="right_attach">4</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="expire_date">
                <property name="data_type">date</property>
                <property name="model_attribute">expire_date</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="left_attach">3</property>
                <property name="right_attach">4</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="client_name">
                <property name="data_type">unicode</property>
                <property name="model_attribute">client_name</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
                <property name="xpad">6</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="left_attach">3</property>
                <property name="right_attach">4</property>
                <property name="top_attach">2</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="padding">6</property>
          </packing>
        </child>
