<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="LoanDetails">
    <property name="default_height">250</property>
    <property name="default_width">440</property>
    <property name="mnemonics_visible">False</property>
    <child>
      <object class="GtkVBox" id="details_vbox">
        <property name="visible">True</property>
        <child>
          <object class="GtkTable" id="table1">
            <property name="border_width">6</property>
            <property name="column_spacing">6</property>
            <property name="n_columns">4</property>
            <property name="n_rows">3</property>
            <property name="row_spacing">6</property>
            <property name="visible">True</property>
            <child>
              <object class="ProxyLabel" id="kiwilabel1">
                <property name="label" translatable="yes">Data de abertura:</property>
                <property name="model_attribute">kiwilabel1</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="x_options">fill</property>
                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel3">
                <property name="label" translatable="yes">Filial:</property>
                <property name="model_attribute">kiwilabel3</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="top_attach">1</property>
                <property name="x_options">fill</property>
                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel5">
                <property name="label" translatable="yes">Responsável:</property>
                <property name="model_attribute">kiwilabel5</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="top_attach">2</property>
                <property name="x_options">fill</property>
                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="open_date">
                <property name="data_type">date</property>
                <property name="ellipsize">start</property>
                <property name="model_attribute">open_date</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
                <property name="xpad">6</property>
              </object>
              <packing>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="branch_name">
                <property name="data_type">unicode</property>
                <property name="model_attribute">branch_name</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
                <property name="xpad">6</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="responsible_name">
                <property name="data_type">unicode</property>
                <property name="model_attribute">responsible_name</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
                <property name="xpad">6</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="label" translatable="yes">Data de confirmação:</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="left_attach">2</property>
                <property name="right_attach">3</property>
                <property name="x_options">fill</property>
                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label2">
                <property name="label" translatable="yes">Data de expiração:</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="left_attach">2</property>
                <property name="right_attach">3</property>
                <property name="top_attach">1</property>
                <property name="x_options">fill</property>
                <property name="y_options">expand</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel6">
                <property name="label" translatable="yes">Cliente:</property>
                <property name="model_attribute">kiwilabel6</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="left_attach">2</property>
                <property name="right_attach">3</property>
                <property name="top_attach">2</property>
