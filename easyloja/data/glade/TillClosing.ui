<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="TillClosing">
    <property name="default_height">400</property>
    <property name="default_width">600</property>
    <property name="title" translatable="yes">Till Closing</property>
    <child>
      <object class="GtkVBox" id="vbox1">
        <property name="spacing">6</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkTable" id="table2">
            <property name="border_width">6</property>
            <property name="column_spacing">6</property>
            <property name="n_columns">2</property>
            <property name="n_rows">2</property>
            <property name="row_spacing">6</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkLabel" id="label9">
                <property name="label" translatable="yes">&lt;b&gt;Detalhes gerais:&lt;/b&gt;</property>
                <property name="use_markup">True</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
              </object>
            </child>
            <child>
              <object class="GtkHBox" id="hbox1">
                <property name="spacing">6</property>
                <property name="visible">True</property>
                <child>
                  <object class="GtkLabel" id="label11">
                    <property name="label" translatable="yes">Data de abertura:</property>
                    <property name="visible">True</property>
                    <property name="xalign">0.0</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                  </packing>
                </child>
                <child>
                  <object class="ProxyLabel" id="opening_date">
                    <property name="data_type">date</property>
                    <property name="model_attribute">opening_date</property>
                    <property name="visible">True</property>
                    <property name="xalign">0.0</property>
                  </object>
                  <packing>
                    <property name="position">1</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="top_attach">1</property>
                <property name="x_options">fill</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
          </packing>
        </child>
        <child>
          <object class="GtkVBox" id="day_history_box">
            <property name="spacing">6</property>
            <property name="visible">True</property>
            <child>
              <object class="ObjectList" id="day_history">
                <property name="border_width">6</property>
                <property name="visible">True</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkTable" id="table1">
            <property name="column_spacing">6</property>
            <property name="n_columns">2</property>
            <property name="n_rows">3</property>
            <property name="row_spacing">6</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkLabel" id="label15">
                <property name="label" translatable="yes">&lt;b&gt;Quantia:&lt;/b&gt;</property>
                <property name="use_markup">True</property>
                <property name="visible">True</property>
                <property name="xalign">0.0</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label18">
                <property name="label" translatable="yes">Fundo de caixa:</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="label17">
                <property name="label" translatable="yes" comments="Do not translate $CURRENCY">Quantia retirada ($CURRENCY):</property>
                <property name="model_attribute">label17</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="top_attach">2</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="balance">
                <property name="data_type">currency</property>
                <property name="model_attribute">balance</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">2</property>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyEntry" id="value">
                <property name="data_type">currency</property>
                <property name="model_attribute">value</property>
                <property name="mandatory">True</property>
                <property name="visible">True</property>
                <property name="width_chars">6</property>
                <property name="xalign">1.0</property>
              </object>
              <packing>
                <property name="bottom_attach">3</property>
                <property name="left_attach">1</property>
                <property name="right_attach">2</property>
                <property name="top_attach">2</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
