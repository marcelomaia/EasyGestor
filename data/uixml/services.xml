<menubar action="menubar">
  <menu action="FileMenu">
    <menu action="NewMenu">
      <placeholder name="NewMenuItemPH">
        <menuitem action="NewOrder"/>
      </placeholder>
    </menu>
    <placeholder name="FileMenuPH">
      <menuitem action="SendOrders"/>
      <menuitem action="ReceiveOrders"/>
    </placeholder>
  </menu>
  <menu action="SearchMenu">
    <placeholder name="SearchMenuPH">
      <menuitem action="Products"/>
      <menuitem action="Services"/>
      <menuitem action="Categories"/>
      <menuitem action="Clients"/>
    </placeholder>
  </menu>
  <menu action="ViewMenu">
    <placeholder name="ViewMenuPH">
      <menuitem action="ViewKanban"/>
      <menuitem action="ViewList"/>
    </placeholder>
  </menu>
  <placeholder name="AppMenubarPH">
    <menu action="OrderMenu">
      <menuitem action="Details"/>
      <separator/>
      <menuitem action="Edit"/>
      <menuitem action="Finish"/>
      <menuitem action="Cancel"/>
      <menuitem action="DeliverOrder"/>
      <separator/>
      <menuitem action="Approve"/>
      <menuitem action="Pause"/>
      <menuitem action="Work"/>
      <menuitem action="Reject"/>
      <menuitem action="UndoRejection"/>
      <menuitem action="Reopen"/>
      <separator/>
      <menuitem action="PrintQuote"/>
      <menuitem action="PrintReceipt"/>
    </menu>
  </placeholder>
</menubar>
<popup name="ServicesSelection">
  <placeholder name="ServicesSelectionPH">
    <menuitem action="Details"/>
    <separator/>
    <menuitem action="Edit"/>
    <menuitem action="Finish"/>
    <menuitem action="Cancel"/>
    <menuitem action="DeliverOrder"/>
    <separator/>
    <menuitem action="Approve"/>
    <menuitem action="Pause"/>
    <menuitem action="Work"/>
    <menuitem action="Reject"/>
    <menuitem action="UndoRejection"/>
    <menuitem action="Reopen"/>
    <separator/>
    <menuitem action="PrintQuote"/>
    <menuitem action="PrintReceipt"/>
  </placeholder>
</popup>
<toolbar action="toolbar">
  <placeholder name="AppToolbarPH">
    <separator/>
    <toolitem action="Edit"/>
    <toolitem action="Finish"/>
    <toolitem action="Cancel"/>
    <toolitem action="Details"/>
  </placeholder>
</toolbar>
